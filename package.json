{
  "name": "deepforge",
  "repository": {
    "type": "git",
    "url": "https://github.com/deepforge-dev/deepforge.git"
  },
  "bin": {
    "deepforge": "./bin/deepforge"
  },
  "scripts": {
    "start": "node ./bin/deepforge start",
    "postinstall": "node utils/reinstall-extensions.js && node utils/build-job-utils.js && ./bin/deepforge create-env",
    "start-dev": "NODE_ENV=dev node ./bin/deepforge start",
    "start-authenticated": "NODE_ENV=production ./bin/deepforge start",
    "server": "node ./bin/deepforge start --server",
    "no-conda-server": "node ./bin/deepforge start --server --no-conda",
    "test": "npm run test-unit && npm run test-int",
    "test-unit": "NODE_ENV=test mkdir ./test-tmp; mocha --recursive test/unit",
    "test-int": "NODE_ENV=test mkdir ./test-tmp; mocha --recursive test/integration",
    "test-func": "bash ./test/features/start.sh",
    "test-ci": "npm test && node utils/additional-ci-checks.js",
    "watch-test": "nodemon --exec 'mocha --recursive test'"
  },
  "version": "2.3.1",
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/runtime": "^7.7.2",
    "aws-sdk": "^2.680.0",
    "chance": "^1.1.4",
    "commander": "^2.20.3",
    "deepforge-user-management-page": "github:deepforge-dev/user-management-page",
    "dotenv": "^2.0.0",
    "exists-file": "^2.1.0",
    "express": "^4.14.0",
    "graceful-fs": "^4.1.10",
    "js-yaml": "^3.13.1",
    "lodash.difference": "^4.1.2",
    "lodash.merge": "^4.5.1",
    "lodash.template": "^4.4.0",
    "minimatch": "^3.0.4",
    "mongodb": "^2.2.10",
    "node-fetch": "^2.6.0",
    "pacote": "^11.1.10",
    "q": "1.4.1",
    "rimraf": "^2.4.0",
    "tcp-port-used": "^0.1.2",
    "webgme": "git+https://github.com/webgme/webgme.git",
    "webgme-autoviz": "^2.2.2",
    "webgme-breadcrumbheader": "^2.1.1",
    "webgme-chflayout": "^2.0.1",
    "webgme-cli": "^2.7.3",
    "webgme-easydag": "github:dfst/webgme-easydag",
    "webgme-fab": "github:dfst/webgme-fab",
    "webgme-simple-nodes": "^2.1.3",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "deepforge-worker": "git+https://github.com/deepforge-dev/worker.git",
    "jszip": "^2.5.0",
    "mocha": "^2.2.5",
    "mockery": "^1.7.0",
    "nodemon": "^1.9.2",
    "skulpt": "^0.11.1"
  }
}
